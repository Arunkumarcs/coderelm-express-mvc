{
  "name": "coderelm-express-mvc",
  "version": "2.0.0",
  "description": "Supercharged Express Generator with Express MVC structure integrated with Sequalize ORM and Nunjucks template engine along with Jest and Puppeteer test framework integrated from Coderelm Team.",
  "main": "./bin/www",
  "repository": "https://github.com/Arunkumarcs/express-mvc.git",
  "author": "Arunkumar <arunkumarcoderelm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./bin/www",
    "start-test": "cross-env NODE_ENV=test node ./bin/www",
    "start-pro": "cross-env NODE_ENV=production node ./bin/www",
    "strat-pm2": "pm2 start",
    "restart-pm2": "pm2 restart expCodeRelm",
    "stop-pm2": "pm2 stop expCodeRelm",
    "test": "./node_modules/.bin/jest",
    "assets-es6": "cd src/es6 && yarn build",
    "assets-react": "cd src/react && yarn build",
    "assets-vue": "cd src/vue && yarn build"
  },
  "dependencies": {
    "@arunkumarcoderelm/use": "^1.0.1",
    "apollo-server-express": "^2.5.0",
    "config": "^3.0.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "csurf": "^1.9.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-graphql": "^0.8.0",
    "express-session": "^1.16.1",
    "faker": "^4.1.0",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.4",
    "helmet": "^3.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "^2.24.0",
    "mongoose": "^5.5.12",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.5",
    "nunjucks": "^3.2.0",
    "redux": "^4.0.1",
    "rotating-file-stream": "^1.4.0",
    "sequelize": "^5.8.5",
    "sequelize-cli": "^5.4.0",
    "session-file-store": "^1.2.0",
    "socket.io": "^2.2.0"
  },
  "keywords": [
    "MVC",
    "nunjucks",
    "sequelize",
    "express",
    "expressjs",
    "es6"
  ],
  "autoload": {
    "Controller": "app/Controller/",
    "Event": "app/Event/",
    "Library": "app/Library/",
    "Middleware": "app/Middleware/",
    "Model": "app/Model/",
    "Routes": "app/Routes/",
    "Socket": "app/Socket/",
    "Provider": "app/Provider/",
    "Config": "app/Config/",
    "App": "app/",
    "Core": "core/",
    "Plugins": "plugins/",
    "Public": "public/",
    "Resources": "resources/",
    "Db": "db/"
  },
  "devDependencies": {
    "express-debug": "^1.1.1",
    "jest": "^24.7.1",
    "puppeteer": "^1.16.0"
  }
}
